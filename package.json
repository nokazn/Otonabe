{
  "name": "ravy",
  "version": "0.1.0",
  "private": true,
  "description": "",
  "license": "MIT",
  "author": {
    "name": "nokazn",
    "url": "https://github.com/nokazn"
  },
  "scripts": {
    "analyze": "nuxt-ts build --analyze",
    "build": "nuxt-ts build",
    "dev": "nuxt-ts",
    "generate": "nuxt-ts generate",
    "lint": "eslint --ext .ts,.js,.vue --ignore-path .eslintignore .",
    "lint:fix": "npm run lint -- --fix",
    "stylelint": "stylelint --ignore-path .stylelintignore \"**/*.{css,scss,vue}\"",
    "stylelint:fix": "npm run lint -- --fix",
    "tsc": "tsc --noEmit",
    "vtc": "vtc --workspace ./ --srcDir ./client/",
    "test": "npm run test:client && npm run test:server",
    "test:client": "jest ./client/**",
    "test:server": "jest ./server/** --passWithNoTests",
    "start": "nuxt-ts start",
    "steps": "find . -type f -not -path \"**/node_modules/**\" -not -path \"./.nuxt/**\" -not -path \"**/dist/**\" | grep -e '.+(\\|\\.ts$\\|\\.js$\\|\\.vue$\\|\\.scss$\\|\\.json\\|)' | xargs wc -l",
    "steps:ts": "find . -type f -not -path \"**/node_modules/**\" -not -path \"./.nuxt/**\" -not -path \"**/dist/**\" | grep -e '\\.ts$' | xargs wc -l",
    "steps:vue": "find . -type f -not -path \"**/node_modules/**\" -not -path \"./.nuxt/**\" -not -path \"**/dist/**\" | grep -e '\\.vue$' | xargs wc -l"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run tsc && npm run stylelint",
      "pre-push": "npm run test"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/axios": "^5.12.2",
    "@nuxtjs/dotenv": "^1.4.1",
    "@nuxtjs/pwa": "^3.2.2",
    "@nuxtjs/style-resources": "^1.0.0",
    "@vue/composition-api": "^1.0.0-beta.19",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "dayjs": "^1.9.6",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "ioredis": "^4.16.0",
    "lodash": "^4.17.20",
    "node-vibrant": "^3.1.6",
    "nuxt": "^2.14.7",
    "nuxt-property-decorator": "^2.8.8",
    "portal-vue": "^2.1.7",
    "spinkit": "^2.0.1",
    "v-snackbars": "^3.1.0",
    "vue": "^2.6.12",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
