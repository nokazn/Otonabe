{
  "name": "@ravyd/server",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "main": "dist/bootstrap.js",
  "scripts": {
    "lint": "eslint --ext .ts,.js --ignore-path ../../.eslintignore .",
    "lint:fix": "yarn lint --fix",
    "tsc": "tsc --noEmit",
    "test": "jest -c ../../jest.config.js --passWithNoTests ./**",
    "dev": "cross-env NODE_ENV=development ts-node-dev -r tsconfig-paths/register --respawn --files ./bootstrap",
    "build": "tsc --build tsconfig.json .",
    "start": "cross-env NODE_ENV=production node -r module-alias/register ../../dist/server/bootstrap"
  },
  "dependencies": {
    "@ravyd/shared": "^0.1.0",
    "connect-redis": "^5.0.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.9.7",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "helmet": "^4.4.1",
    "http-status-codes": "^2.1.4",
    "ioredis": "^4.19.2",
    "lodash": "^4.17.20",
    "module-alias": "^2.2.2",
    "url-join": "^4.0.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@types/connect-redis": "^0.0.16",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.9",
    "@types/express-session": "^1.17.3",
    "@types/helmet": "^4.0.0",
    "@types/http-status-codes": "^1.2.0",
    "@types/ioredis": "^4.17.8",
    "@types/lodash": "^4.14.165",
    "@types/log4js": "^2.3.5",
    "@types/node": "^14.14.12",
    "@types/url-join": "^4.0.0",
    "@types/uuid": "^8.3.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "ts-node-dev": "^1.0.0",
    "tsconfig-paths": "^3.9.0"
  }
}
